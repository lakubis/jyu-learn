   "source": [
    "function fib(c::Channel)\n",
    "    a = 0\n",
    "    b = 1\n",
    "    put!(c, a)\n",
    "    while true\n",
    "        put!(c, b)\n",
    "        (a, b) = (b, a+b)\n",
    "    end\n",
    "end"
   ]
  }
